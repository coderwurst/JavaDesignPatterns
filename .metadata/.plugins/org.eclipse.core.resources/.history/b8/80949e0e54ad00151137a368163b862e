package com.coderwurst.adapter_patterns;

import java.util.ArrayList;
import java.util.List;

import com.coderwurst.design_patterns.Engine;

public class Simulator {
	
	List<Engine> engineList = new ArrayList<Engine>();
	engineList.add(new StandardEngine (1200));
	engineList.add(new StandardEngine (1600));
	engineList.add(new TurboEngine (2000));
	engineList.add(new TurboEngine (2200));
	
	SuperGreenEngine superGreenEngine = new SuperGreenEngine(1500);
	engineList.add(new SuperGreenEngineAdapter(superGreenEngine));
	
	for (engine:engineList){
		System.out.println(engine);
	}
	

}
